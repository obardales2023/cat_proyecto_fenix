    	<var name="GUID_PERSONA" expr="AppState.var_datos_cliente_guid" />	
		<block>
			<data name="BKND_CrearSesionCertData" src="../src/CrearSesionCert.jsp" method="post"  enctype="multipart/form-data"
				namelist="AppStateString CallID Token URL CLIENTE_ID GUID_PERSONA" />
        	<if cond="typeof BKND_CrearSesionCertData.errorMsg == 'undefined'">
        		<assign name="AppState.BKND_CrearSesionCertResult" expr="BKND_CrearSesionCertData" />
            	<if cond="typeof AppState.BKND_CrearSesionCertResult.HttpCode != 'undefined'">
            		<assign name="AppState.var_crear_sesion_http" expr="AppState.BKND_CrearSesionCertResult.HttpCode" />
            	</if>
            	<if cond="typeof AppState.BKND_CrearSesionCertResult.sesion_id != 'undefined'">
            		<assign name="AppState.var_crear_sesion_id_sesion" expr="AppState.BKND_CrearSesionCertResult.sesion_id" />
            	</if>
				<goto next="#CrearSesionCert_HttpCode201" />
        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="BKND_CrearSesionCertData.errorMsg" />
        	</if>
		</block>
		
		<catch event="error.badfetch">
			<goto next="#LogErrorCreaSesion" />
    	</catch>
		<catch event="error.com.genesyslab.composer.servererror">
			<goto next="#LogErrorCreaSesion" />
    	</catch>
	</form>

	<!-- This is the Assign block -->
	<form id="AsignaClientId">
		<block>
			
			<assign name="AppState.clientId" expr="AppState.var_RUT + '-CHL-1'" />
			<assign name="AppState.var_flag_crear_sesion" expr="0" />
			<assign name="AppState.var_cliente" expr="'Cliente'" />				
			<goto next="#Backend2" />
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="LogErrorCreaSesion">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
