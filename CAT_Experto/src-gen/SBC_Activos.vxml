		<block>
			<data name="Backend4Data" src="../src/Logger.jsp" method="post"  enctype="multipart/form-data"
				namelist="AppStateString LoggerMensaje LoggerNivel" />
        	<if cond="typeof Backend4Data.errorMsg == 'undefined'">
        		<assign name="AppState.Backend4Result" expr="Backend4Data" />
				<goto next="#BKND_ConsultaActivos" />
        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="Backend4Data.errorMsg" />
        	</if>
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="Backend5">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
    	<var name="LoggerMensaje" expr="'Activos -&gt;OK'" />
    	<var name="LoggerNivel" expr="'Info'" />	
		<block>
			<data name="Backend5Data" src="../src/Logger.jsp" method="post"  enctype="multipart/form-data"
				namelist="AppStateString LoggerMensaje LoggerNivel" />
        	<if cond="typeof Backend5Data.errorMsg == 'undefined'">
        		<assign name="AppState.Backend5Result" expr="Backend5Data" />
				<goto next="#Exit1" />
        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="Backend5Data.errorMsg" />
        	</if>
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="BKND_ConsultaOfertaClientes">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
    	<var name="CallID" expr="AppState.CallUUID" />
    	<var name="Token" expr="AppState.var_GetToken_Token" />
    	<var name="URL" expr="AppState.var_ConsultaOfertas_url" />
    	<var name="rutEncriptado" expr="AppState.rut_encriptado" />	
		<block>
			<data name="BKND_ConsultaOfertaClientesData" src="../src/ConsultaOfertaClientes.jsp" method="post"  enctype="multipart/form-data"
				namelist="AppStateString CallID Token URL rutEncriptado" />
        	<if cond="typeof BKND_ConsultaOfertaClientesData.errorMsg == 'undefined'">
        		<assign name="AppState.BKND_ConsultaOfertaClientesResult" expr="BKND_ConsultaOfertaClientesData" />
            	<if cond="typeof AppState.BKND_ConsultaOfertaClientesResult.HttpCode != 'undefined'">
            		<assign name="AppState.var_ConsultaOfertas_http" expr="AppState.BKND_ConsultaOfertaClientesResult.HttpCode" />
            	</if>
            	<if cond="typeof AppState.BKND_ConsultaOfertaClientesResult.idCliente != 'undefined'">
            		<assign name="AppState.id_Cliente" expr="AppState.BKND_ConsultaOfertaClientesResult.idCliente" />
            	</if>
				<goto next="#ConsultaOferCliCert_HttpCode200" />
        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="BKND_ConsultaOfertaClientesData.errorMsg" />
        	</if>
