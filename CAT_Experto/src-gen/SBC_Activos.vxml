		<var name="AppStateString" expr="JSON.stringify(AppState)" />
    	<var name="LoggerMensaje" expr="AppState.var_HdrLog + ' [WS4][Cerrar Sesion CRM] json result: ' +  ', http code: ' + AppState.var_cerrar_sesion_http" />
    	<var name="LoggerNivel" expr="'Info'" />	
		<block>
			<data name="BKND_LogCerrarData" src="../src/Logger.jsp" method="post"  enctype="multipart/form-data"
				namelist="AppStateString LoggerMensaje LoggerNivel" />
        	<if cond="typeof BKND_LogCerrarData.errorMsg == 'undefined'">
