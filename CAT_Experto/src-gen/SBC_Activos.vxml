		<catch event="error.com.genesyslab.composer.servererror">
			<goto next="#LogErrorConsultaOferCli" />
    	</catch>
	</form>

	<!-- This is the Branching block -->
	<form id="ConsultaOferCliCert_HttpCode200">
		<block>
			<if cond="AppState.var_ConsultaOfertas_http != 200 || AppState.var_ConsultaOfertas_http == -1">
				
				<!-- This is the OutLink -->
				<goto next="#GetActivos_0" />
			</if>
			<goto next="#Copy_1_Backend3" />
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="LogErrorConsultaOferCli">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
    	<var name="LoggerMensaje" expr="AppState.var_HdrLog + ' Error en Consultar Activos'" />
