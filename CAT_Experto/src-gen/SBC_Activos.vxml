        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="BKND_LogCerrarData.errorMsg" />
        	</if>
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_marcaCortaLlamada">
		<block>
			
			<assign name="AppState.var_log_navegacion" expr="AppState.var_log_navegacion.concat(',ClienteCorta')" />				
			<goto next="#ASG_marcaEstadoLlamada" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_marcaError">
		<block>
			
			<assign name="AppState.var_log_navegacion" expr="AppState.var_log_navegacion.concat(',errorLlamada')" />				
			<goto next="#ASG_marcaEstadoLlamada" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_marcaEstadoLlamada">
		<block>
			
			<assign name="AppState.EstadoError" expr="1" />				
			<goto next="#Exit1" />
		</block>
	</form>

	<!-- This is the Branching block -->
	<form id="ValidaVIP_Cliente">
		<block>
			<if cond="AppState.esVIP == 1">
				
				<!-- Post Action -->
				<script>try{
				AppState.var_cliente = 'VIP'
				}
				catch(err){}
				
				</script>
				
				<!-- This is the OutLink -->
				<goto next="#Exit1" />
			</if>
			<goto next="#AsignaClientId" />
		</block>
	</form>

	<!-- This is the Branching block -->
	<form id="ValidaVIP_NoCliente">
		<block>
			<if cond="AppState.esVIP == 1">
				
				<!-- Post Action -->
				<script>try{
				AppState.var_cliente = 'VIP'
				}
				catch(err){}
				
				</script>
				
				<!-- This is the OutLink -->
				<goto next="#Exit1" />
			</if>
			<goto next="#NoCliente" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="NoCliente">
		<block>
			
			<assign name="AppState.var_cliente" expr="'No Cliente'" />
			<assign name="AppState.esCliente" expr="0" />				
			<goto next="#Exit1" />
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="BKND_ProceoInicio">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
