				<goto next="#BKND_CrearSesionCert" />
        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="Backend2Data.errorMsg" />
        	</if>
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="Backend3">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
    	<var name="LoggerMensaje" expr="'Activos -&gt;Crear Sesion OK'" />
    	<var name="LoggerNivel" expr="'Info'" />	
		<block>
			<data name="Backend3Data" src="../src/Logger.jsp" method="post"  enctype="multipart/form-data"
				namelist="AppStateString LoggerMensaje LoggerNivel" />
        	<if cond="typeof Backend3Data.errorMsg == 'undefined'">
        		<assign name="AppState.Backend3Result" expr="Backend3Data" />
				<goto next="#BKND_ConsultaOfertaClientes" />
        	<else/>
        		<throw event="error.com.genesyslab.composer.servererror" messageexpr="Backend3Data.errorMsg" />
        	</if>
		</block>
	</form>

	<!-- This is the Backend logic block -->
	<form id="Backend4">

		<var name="AppStateString" expr="JSON.stringify(AppState)" />
