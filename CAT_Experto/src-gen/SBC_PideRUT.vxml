			<assign name="AppState.var_cont_rut1" expr="AppState.var_cont_rut1 + 1" />				
			<goto next="#BRCH_RUT1" />
		</block>
	</form>

	<!-- This is the Branching block -->
	<form id="BRCH_RUT1">
		<block>
			<if cond="AppState.var_cont_rut1 &gt;= 3">
				
				<!-- Post Action -->
				<script>try{
				AppState.var_TranferVDN = &quot;12345&quot;
				}
				catch(err){}
				
				</script>
				
				<!-- This is the OutLink -->
				<goto next="#PRM_RutInvalido1" />
			</if>
			<goto next="#PRM_RutInvalido" />
		</block>
	</form>

	<!-- This is the Prompt block -->
	<form id="PRM_RutInvalido1">
        
    	<block name="PRM_RutInvalido1P0">
  			<prompt bargein="true"  gvp:langexpr="AppState.APP_LANGUAGE">
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'Audios Experto/Usted ha superado el nÃºmero de ingresos.wav'"></audio>
    		</prompt>
		</block>
    	<block>
    		<goto next="#ASG_MaximoIntentos1" />
    	</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_MaximoIntentos1">
		<block>
			
			<assign name="AppState.var_log_navegacion" expr="AppState.var_log_navegacion.concat(',RUT_nok_3intentos_corta')" />
			<assign name="AppState.varAccion" expr="'cortar'" />
			<assign name="AppState.estado" expr="1" />				
			<goto next="#Salida" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_marcaCortaLlamada">
		<block>
			
			<assign name="AppState.var_log_navegacion" expr="AppState.var_log_navegacion.concat(',ClienteCorta')" />				
			<goto next="#ASG_marcaEstadoLlamada" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_marcaError">
		<block>
			
			<assign name="AppState.var_log_navegacion" expr="AppState.var_log_navegacion.concat(',errorLlamada')" />				
			<goto next="#ASG_marcaEstadoLlamada" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ASG_marcaEstadoLlamada">
		<block>
			
			<assign name="AppState.EstadoError" expr="1" />				
			<goto next="#Salida" />
		</block>
	</form>

</vxml>
